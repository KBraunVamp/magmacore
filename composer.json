{
    "name": "koehl/magmacore",
    "description": "Simple PHP framework for building web applications",
    "keywords": ["framework", "php", "magmacore"],
    "type": "library",
    "license": "MIT",
    "readme": "",
    "time": "",
    "support": {
        "issues": "https://github.com/KBraunVamp/magmacore/issues",
        "source": "https://github.com/KBraunVamp/magmacore",
        "wiki": "https://github.com/KBraunVamp/magmacore/wiki"
        
    },    
    "authors": [
        {
            "name": "KBraunVamp",
            "email": "kalinbraunvamp@gmail.com"
        }
    ],
    "minimum-stability": "alpha",
    "require": {
        "php": "7.4.19|^8.0",
        "ext-mbstring": "*",
        "ext-iconv": "*",
        "ext-pdo": "*",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-simplexml": "*",
        "twig/twig": "^3.3",
        "symfony/http-foundation": "^5.4",
        "symfony/yaml": "^6.0"        
    },
    "autoload": {
        "psr-4": {
            "Magma\\": "src/Magma"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Magma\\Tests\\": "tests"
        }
    }
